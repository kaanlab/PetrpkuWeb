{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "SqLiteConnection": "DataSource=app.db",
    "MsSqlConnection": "Persist Security Info=False;Server=mssql.pku.petrpku.ru;Initial Catalog=petrpkuweb;User ID=petrpkuweb_admin; Password=12345"
  },
  "ldap": {
    "url": "pku.petrpku.ru",
    "bindDn": "DC=pku,DC=petrpku,DC=ru",
    "username": "readuser",
    "password": "Read123456",
    "searchBase": "OU=PKU,DC=pku,DC=petrpku,DC=ru",
    "searchOneFilter": "(&(objectClass=user)(objectClass=person)(sAMAccountName={0}))",
    "searchAllFilter": "(&(objectClass=user)(objectClass=person)(sAMAccountName=*)(!(ou=students)))"
  },
  "Jwt": {
    "Key": "SOME_RANDOM_KEY_DO_NOT_SHARE",
    "Issuer": "http://test.pku.petrpku.ru/",
    "Audience": "http://test.pku.petrpku.ru/",
    "ExpireDays": 30
  },
  "AllowedHosts": "*"
}
